    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^20.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.3",
